
.caregory{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  height: 100%;
}
.nav_left{
  background: #f8f8f8;
  width: 24%;
  height: 100%;
  white-space: nowrap;
}
.nav_right{
  background: #fff;
  width: 76%;
  height: 100%;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.nav_left_items{
  font-size: 28rpx;
  color: #333;
  height: 100rpx;
  padding-top:25rpx;
  padding-bottom:25rpx;
  line-height: 100rpx;
  text-align: center;
}
.nav_left_item_active{
  background-color: #fff;
  color: #e93b3d;
}
.nav_left_items:active{
	backgound-color:#fff!important;
}
page{
	height:100%
}
.nav_right_items{
	display:none;
	width:40%;
	padding-left:5%;
	padding-right:5%;
	height:140px;
	float:right;
}
.nav_right_items image{

	width:50px;
	height:50px;
	padding-left: 45%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding-top: 46px;
}
.nav_right_item_active{
	display:block;
}
@font-face {
	font-family:'set_font';
	src:url(/static/font/优设标题黑.ttf) format('truetype');
}
text{
	font-family: 'set_font',常规体;
}

